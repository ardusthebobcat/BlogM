<%= form_for @user do |t| %>
  <%= t.label :first %>
  <%= t.text_field :first %><br>
  <%= t.label :last %>
  <%= t.text_field :last %><br>
  <%= t.label :email %>
  <%= t.text_field :email %><br>
  <%= t.label :password %>
  <%= t.text_field :password %><br>
  <%= t.label :password_confirmation %>
  <%= t.text_field :password_confirmation %><br>

  <%= t.submit %>

<% end %>
